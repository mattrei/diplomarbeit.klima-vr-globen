    "nunjucks-html-loader": "^1.1.0",
    "semistandard": "^11.0.0",
    "serve": "^6.5.5",
    "snazzy": "*",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
